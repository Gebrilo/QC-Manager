# TestSprite MCP Integration Configuration
# Copy this file to .env and add your TestSprite API key

# TestSprite API Key (from your Claude Desktop config)
TESTSPRITE_API_KEY=your_api_key_here

# Webhook endpoint (where TestSprite should send results)
TESTSPRITE_WEBHOOK_URL=http://localhost:3001/testsprite/webhook

# Default project ID for automated test uploads (optional)
TESTSPRITE_DEFAULT_PROJECT_ID=

# Enable/disable TestSprite integration
TESTSPRITE_ENABLED=true

# TestSprite result format
# Options: auto, jest, mocha, generic
TESTSPRITE_RESULT_FORMAT=auto
